{
  "name": "telwagen-backend",
  "version": "1.0.0",
  "description": "Backend API para el Generador de Facturas Telwagen",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "dev": "nodemon server.js",
    "dev:stable": "node server.js",
    "dev:watch": "nodemon --config nodemon.json server.js",
    "migrate:postgresql": "node migrar_a_postgresql.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop telwagen-backend",
    "pm2:restart": "pm2 restart telwagen-backend",
    "pm2:logs": "pm2 logs telwagen-backend",
    "pm2:monit": "pm2 monit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nodejs",
    "express",
    "sqlite",
    "facturas",
    "telwagen"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "edge-js": "^24.0.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.1.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "pg": "^8.11.3",
    "selfsigned": "^3.0.1",
    "sqlite3": "^5.1.6",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
